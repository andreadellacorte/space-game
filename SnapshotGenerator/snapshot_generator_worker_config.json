{
  "workerType": "SnapshotGenerator",
  "layer": "client",
  "entityInterest": {
    "rangeEntityInterest": {
      "radius": 2
    }
  },
  "componentDelivery": {
    "default": "RELIABLE_ORDERED",
    "checkoutAllInitially": true
  },
  "uploadConfiguration": {
    "windows": {
      "localBuildDirectory": "../SnapshotGenerator/bin/x64/ReleaseWindows"
    },
    "macos": {
      "localBuildDirectory": "../SnapshotGenerator/bin/x64/ReleaseMacOS"
    },
    "linux": {
      "localBuildDirectory": "../SnapshotGenerator/bin/x64/ReleaseLinux"
    }
  },
  "permissions": {
    "entity_creation": {
        "allow": true
    }
  }
}

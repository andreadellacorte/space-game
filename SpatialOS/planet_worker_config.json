{
  "workerType": "PlanetWorker",
  "layer": "greetings",
  "entityInterest": {
    "rangeEntityInterest": {
      "radius": 2
    }
  },
  "componentDelivery": {
    "default": "RELIABLE_ORDERED",
    "checkoutAllInitially": true
  },
  "launchConfiguration": {
    "cloudDeployment": {
      "linux": {
        "localBuildDirectory": "../PlanetWorker/bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "PlanetWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    },
    "localDeployment": {
      "windows": {
        "localBuildDirectory": "../PlanetWorker/bin/x64/ReleaseWindows",
        "command": "PlanetWorker.exe",
        "arguments": [
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "macos": {
        "localBuildDirectory": "../PlanetWorker/bin/x64/ReleaseMacOS",
        "command": "mono",
        "arguments": [
          "--arch=64",
          "PlanetWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "linux": {
        "localBuildDirectory": "../PlanetWorker/bin/x64/ReleaseLinux",
        "command": "mono",
        "arguments": [
          "PlanetWorker.exe",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}",
          "${IMPROBABLE_WORKER_ID}"
        ]
      }
    }
  }
}
